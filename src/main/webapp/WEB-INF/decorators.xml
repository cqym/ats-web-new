<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
	</excludes>
	
	<decorator name="selectList" page="selectList.jsp">
		<pattern>/module/select</pattern>
		<pattern>/xmlConfig/select</pattern>
	</decorator>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
